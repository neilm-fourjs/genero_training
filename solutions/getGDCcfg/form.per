ATTRIBUTES
GROUP grp1 : grp1, TEXT=%"Details";

LABEL l001 : l_file_rmt,TEXT=%"Remote File:", JUSTIFY=RIGHT,STYLE="bold";
BUTTONEDIT  n001 = FORMONLY.m_file_remote, SCROLL, ACTION=fileopen;

LABEL l002 : l_file_lcl,TEXT=%"Local File:";
EDIT  n002 = FORMONLY.m_file_local;

TABLE apps : apps, DOUBLECLICK=details;

IMAGE img  = FORMONLY.f1,TITLE=%"Icon";
EDIT  f001 = FORMONLY.f2,TITLE=%"Name";
EDIT  f002 = FORMONLY.f3,TITLE=%"Host";
EDIT  f003 = FORMONLY.type,TITLE=%"Type";
EDIT  f004 = FORMONLY.user,TITLE=%"User";
EDIT  f005 = FORMONLY.command,TITLE=%"Command";
END 

