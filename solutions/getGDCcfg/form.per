
TOPMENU
	GROUP file (TEXT=%"File")
		COMMAND fileopen
		SEPARATOR
		COMMAND exit
	END -- GROUP
	GROUP edit (TEXT=%"Edit")
		COMMAND editcopy
		COMMAND editcut
		COMMAND editpaste
	END -- GROUP
	GROUP val (TEXT=%"Validation")
		COMMAND accept
		COMMAND cancel
	END -- GROUP
	GROUP det (TEXT=%"Details")
		COMMAND  details
	END -- GROUP
	GROUP help (TEXT=%"Help")
		COMMAND  about
	END -- GROUP
END -- TOPMENU

TOOLBAR ( BUTTONTEXTHIDDEN )
		ITEM exit
		SEPARATOR
		ITEM accept
		ITEM cancel
		SEPARATOR
		ITEM prevrow
		ITEM nextrow
		SEPARATOR
		ITEM fileopen
		ITEM details
		SEPARATOR
		ITEM about
END -- TOOLBAR

